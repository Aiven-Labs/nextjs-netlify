{
  "name": "aiven-nextjs-netlify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma migrate reset --force && prisma generate && next build",
    "start": "next start",
    "lint": "next lint --ext=.ts,.tsx,.js . && prettier --check '**/*.{ts,tsx,js,json,css}'",
    "format": "prettier --write '**/*.{ts,tsx,js,json,css}' && eslint --fix --ext=.ts,.tsx,.js ."
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aivenio/aquarium": "^1.23.0",
    "@next/font": "^13.4.4",
    "@prisma/client": "^4.15.0",
    "@types/node": "^18.16.16",
    "@types/react": "^17.0.60",
    "@types/react-dom": "^17.0.20",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "ioredis": "^5.3.2",
    "next": "^12.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^2.1.5",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@aivenio/eslint-config-aiven": "^4.0.2",
    "csv-parse": "^5.4.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prisma": "^4.15.0",
    "ts-node": "^10.9.1"
  }
}
